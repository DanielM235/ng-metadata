    "chai": "3.4.0",
    "commitizen": "2.4.6",
    "cz-conventional-changelog": "1.1.4",
    "mocha": "2.3.3",
    "systemjs": "0.19.5",
    "systemjs-builder": "0.14.10",
    "typescript": "1.6.2",
