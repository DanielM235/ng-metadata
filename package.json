{
  "name": "ng-metadata",
  "version": "1.0.0-beta.3",
  "description": "Angular 2 style decorators and utils for Angular 1.x",
  "main": "ng-metadata.js",
  "typings": "./ng-metadata.d.ts",
  "scripts": {
    "typings": "typings install",
    "prepublish": "npm run typings && tsc",
    "start": "tsc -w",
    "test": "mocha ng-metadata.test.js",
    "test:live": "mocha ng-metadata.test.js -w",
    "commit": "git-cz",
    "playground": "tsc -p playground -w & live-server --no-browser"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngParty/ng-metadata.git"
  },
  "keywords": [
    "angularjs",
    "typescript",
    "decorators"
  ],
  "author": "Martin Hochel <hochelmartin@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ngParty/ng-metadata/issues"
  },
  "homepage": "https://github.com/ngParty/ng-metadata#readme",
  "devDependencies": {
    "angular": "1.4.8",
    "chai": "3.4.1",
    "commitizen": "2.4.6",
    "cz-conventional-changelog": "1.1.4",
    "ghooks": "1.0.1",
    "mocha": "2.3.4",
    "systemjs": "0.19.6",
    "systemjs-builder": "0.14.11",
    "typescript": "1.7.3",
    "typings": "0.3.0",
    "validate-commit-msg": "1.0.0"
  },
  "dependencies": {},
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm test"
    }
  }
}
